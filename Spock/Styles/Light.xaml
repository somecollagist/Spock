<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="Background" Color="#F5F5F5"/>
	<SolidColorBrush x:Key="BackgroundDim" Color="#D0D0D0"/>

	<SolidColorBrush x:Key="Foreground" Color="#1B1B1B"/>
	<SolidColorBrush x:Key="ForegroundDim" Color="#404040"/>

	<SolidColorBrush x:Key="Accent" Color="#00E0FF"/>
	<SolidColorBrush x:Key="AccentDim" Color="#00A0BF"/>

	<SolidColorBrush x:Key="Border" Color="#494949"/>
	<SolidColorBrush x:Key="Disabled" Color="#767676"/>
</ResourceDictionary>