<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="Background" Color="#1B1B1B"/>
	<SolidColorBrush x:Key="BackgroundDim" Color="#404040"/>

	<SolidColorBrush x:Key="Foreground" Color="#F5F5F5"/>
	<SolidColorBrush x:Key="ForegroundDim" Color="#D0D0D0"/>

	<SolidColorBrush x:Key="Accent" Color="#D80000"/>
	<SolidColorBrush x:Key="AccentDim" Color="#A00000"/>

	<SolidColorBrush x:Key="Border" Color="#939393"/>
	<SolidColorBrush x:Key="Disabled" Color="#C4C4C4"/>
</ResourceDictionary>